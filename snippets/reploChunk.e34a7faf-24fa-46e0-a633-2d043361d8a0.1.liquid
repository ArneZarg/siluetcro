  --announcement-height: 0px !important;
}

/* keep the theme header group in DOM but out of layout */
.shopify-section-group--header,
[id^="shopify-section"][id*="_header"] {
  display: block !important;
  height: 0 !important;
  min-height: 0 !important;
  overflow: hidden !important;
  visibility: hidden !important;
  position: static !important;
}

/* kill the spacer that creates the jump */
.header__backfill { height: 0 !important; }

/* stop any automatic top spacing on the page wrapper */
html, body, #MainContent, main, .content-for-layout {
  padding-top: 0 !important;
  margin-top: 0 !important;
}

/* neutralize sticky behavior if any inline styles appear */
header[style*="position: sticky"], header[style*="position:fixed"] {
  position: static !important;
  top: auto !important;
}

/* avoid animated jumps */
html, body, header, #MainContent { transition: none !important; }

.shopify-section[data-section-type="announcement"],
[id^="shopify-section"][id*="_announcement"] { display: none !important; height: 0 !important; }

